@model IEnumerable<UGPC_IIUI.Models.Committee>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table" >
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model) {
        <tr>
            <td class="col-md-4">
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td class="col-md-4">
                @Html.ActionLink("Details", "Details", new { id=item.CommitteeId })
            </td>
        </tr>
    }

</table>